# Configurações do Django - COPIE PARA .env EM PRODUÇÃO

# Segurança
# Gere uma chave segura com: python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())'
SECRET_KEY=django-insecure-dev-key-change-in-production

# Ambiente
DEBUG=True
ENVIRONMENT=development

# Hosts permitidos (separe por vírgula)
ALLOWED_HOSTS=localhost,127.0.0.1

# Banco de dados (opcional - padrão SQLite)
DATABASE_URL=
